{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "alwaysStrict": true,
    //   "strict": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "strictPropertyInitialization": false,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "incremental": true,
    "skipLibCheck": true
  }
}
